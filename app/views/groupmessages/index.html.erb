<p id="notice"><%= notice %></p>
<table>
  <thead>
    <tr>
      <th>Text</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @groupmessages.each do |groupmessage| %>
      <tr>
        <td><%= groupmessage.text %></td>
        <td><%= groupmessage.user.name %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<%= render "form", groupmessage:@groupmessage %>
<br>
