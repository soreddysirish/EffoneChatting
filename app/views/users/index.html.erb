<div class="row">
  <!-- Not implemented on tutorial -->
  <div class="col-md-4">
    <div style="height: 300px; overflow-y: auto;">
      <div class="panel panel-default">
        <!-- Default panel contents -->
        <div class="panel-heading">Registered Users</div>

        <!-- Table -->
        <table class="table">
          <thead>
          <tr>
            <th>Name</th>
          </tr>
          </thead>
          <tbody>
          <% @users.each_with_index do |user, index| %>
              <tr>

                  <% if user_signed_in? %>
                <td><%=link_to  user.name,"#",class: " btn-xs start-conversation",
                            "data-sid" => current_user.id, "data-rip" => user.id %></td>
                                  <% end %>
                                    <td><% index +=1 %></td>
              </tr>
          <% end %>

          </tbody>
        </table>
      </div>

    </div>
    <hr>


</div>
</div>
