<%= subscribe_to "/users/new" %>
 <nav class="navbar-default navbar-static-side" role="navigation" style="background-color: #2f4050;">
    <div class="sidebar-collapse">
        <ul class="nav metismenu" id="side-menu" style="display:block;">
            <li class="nav-header">
                <div class="dropdown profile-element">
                    <a data-toggle="dropdown" class="dropdown-toggle" href="#">
                        <span class="clear">
                            <span class="block m-t-xs">
                                <strong class="font-bold">Online users</strong>
                                <% if user_signed_in? %>
                                <%= link_to "Logout", destroy_user_session_path, method: :delete  %>
                                <% else %>
                                <%= link_to "Signin", new_user_session_path, :style => "color:white" %>
                                <% end %>
                            </span>
                        </span>
                    </a>
                </div>
            </li>
	<% @users = User.online_users_list %>
   <% @onlineusers = @users - [current_user] %>
		<% @onlineusers.each do  |user| %>
		<% if user_signed_in? %>
      		<li style="list-style-type: none;" class=".online_users">
      			<%= link_to image_tag(user.avatar) + "&nbsp&nbsp".html_safe + "#{user.name.titleize}", "#",class: " btn-xs start-conversation",
      		"data-sid" => current_user.id, "data-rip" => user.id, :id => "user_image" %>
      	</li>
		<% end %>
		<% end %>
  </ul>
  
	<div>
</nav>

